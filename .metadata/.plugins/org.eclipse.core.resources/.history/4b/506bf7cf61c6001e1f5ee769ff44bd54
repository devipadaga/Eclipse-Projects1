package dailyTasks;

import java.util.Arrays;
import java.util.Collections;

public class Date_8_02_2024 {

	public static void main(String[] args) {
		
		String s="Java is java again java again";
		countElementOccurance(s);
		
		int n=340;
		System.out.println(countDigits(n));
	}
	
	public static void countElementOccurance(String s)
	{
		
        char c = 'g';
 
        System.out.println(Collections.frequency(Arrays.asList(s.split("")),String.valueOf(c)));
	}
	
	public static int countDigits(int n)
	{
		String num=String.valueOf(n);
		return num.length();
	}

}
