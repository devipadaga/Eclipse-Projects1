package VelocityPractice;

import java.util.ArrayList;
import java.util.Collections;
import java.util.HashMap;
import java.util.List;
import java.util.Map;
import java.util.Map.Entry;

public class Secondoccurance {

	public static void main(String[] args) {
		
		String s="Tirumala Devi";
		
	
		Map<Character, Integer> freqs = new HashMap<>();
		 
        // Count the frequencies of characters
        for (char c : s.toCharArray()) {
            freqs.put(c, freqs.getOrDefault(c, 0) + 1);
        }
 
        // Sort the frequencies in descending order
        List<Map.Entry<Character, Integer> > sortedFreqs = new ArrayList<>(freqs.entrySet());
        Collections.sort(
            sortedFreqs,
            (a, b) -> b.getValue().compareTo(a.getValue()));
 
        // Check if there is a second most frequent
        // character
        if (sortedFreqs.size() > 1) {
            System.out.println(sortedFreqs.get(2).getKey());
        }
        else {
            System.out.println("0"); // Return null character if there
                         // is no second most frequent
                         // character
        }
    }
 
		
		
				
	
}
